    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.33.1",
    "@tanstack/react-query": "^4.35.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-use": "^17.4.0",
    "redux-persist": "^6.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.1.6",
    "wretch": "^2.6.0",
    "zod": "^3.22.2",
