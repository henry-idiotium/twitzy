{
  "name": "twitzy",
  "private": true,
  "scripts": {
    "dev": "doppler run next dev",
    "build": "doppler run next build",
    "start": "doppler run next start",
    "lint": "next lint",
    "type:gen": "pnpm sp:gen-type && pnpm prisma:gen-type",
    "prisma:studio": "doppler run prisma studio",
    "prisma:gen-type": "doppler run prisma generate",
    "prisma:migration": "doppler run 'prisma migrate dev --skip-generate'",
    "sp:start": "supabase start",
    "sp:stop": "supabase stop",
    "sp:gen-type": "doppler run --command \"supabase gen types typescript --local --debug > src/types/database.ts\""
  },
  "dependencies": {
