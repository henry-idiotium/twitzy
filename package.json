{
  "name": "twitzy",
  "private": true,
  "scripts": {
    "dev": "doppler run next dev",
    "build": "doppler run next build",
    "start": "doppler run next start",
    "lint": "next lint",
    "type:gen": "bun sp:gen && bun pm:gen",
    "pm:gen": "doppler run prisma generate",
    "pm:mig": "doppler run --command 'prisma migrate dev --skip-generate'",
    "sp:dev": "supabase start",
    "sp:stop": "supabase stop",
    "sp:gen": "doppler run --command 'supabase gen types typescript --local --debug > src/types/database.ts'",
    "shad:add": "bunx shadcn-ui@latest add"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "^5.3.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.33.2",
    "@tanstack/react-query": "^4.35.3",
    "change-case": "^4.1.2",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-use": "^17.4.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.0.8",
    "typescript": "5.2.2",
    "wretch": "^2.7.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@reflet/http": "^1.0.0",
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "encoding": "^0.1.13",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "prisma": "^5.3.0",
    "supabase": "^1.93.0",
    "zod-prisma-types": "^2.7.9"
  },
  "trustedDependencies": [
    "supabase"
  ]
}
